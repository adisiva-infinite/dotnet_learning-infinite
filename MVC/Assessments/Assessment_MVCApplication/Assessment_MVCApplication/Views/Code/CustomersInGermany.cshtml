@model IEnumerable<Assessment_MVCApplication.Models.Customer>
@{
    ViewBag.Title = "CustomersInGermany";
}

<h2>CustomersInGermany</h2>

<table border="1">
    <thead>
        <tr>
            <th>Customer ID</th>
            <th>Company Name</th>
            <th>Contact Name</th>
            <th>Country</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
<tr>
    <td>@customer.CustomerID</td>
    <td>@customer.CompanyName</td>
    <td>@customer.ContactName</td>
    <td>@customer.Country</td>
</tr>}
    </tbody>
</table>

